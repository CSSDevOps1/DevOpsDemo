    <script data-require="angular.js@1.2.21" data-semver="1.2.21" src="https://code.angularjs.org/1.2.21/angular.js"></script>*@
       
    <link href="~/Content/style.css" rel="stylesheet" />
    <script src="~/Scripts/script.js"></script>
    <script src="~/Scripts/smart-table.debug.js"></script>
    <script src="~/Scripts/lrInfiniteScrollPlugin.js"></script>

<link href="~/Content/SMS.css" rel="stylesheet" />
   

<!-- SignalR scripts -->
<script src="~/signalr/hubs"></script>
@Html.IncludeScript("~/Abp/Framework/scripts/libs/abp.signalr.js")

@Scripts.Render("~/Bundles/App/Main/js")
    
