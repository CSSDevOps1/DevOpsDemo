@using System.Threading
@using System.Web.Optimization
@using Abp.Timing
@using Abp.Web.Mvc.Extensions
@{
SetAntiForgeryCookie();
}
<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="">
<meta name="author" content="">

<title>SampleBoilerTemp</title>

<link rel="shortcut icon" href="~/favicon.ico">

@Styles.Render("~/Bundles/App/vendor/css")
@Styles.Render("~/Bundles/App/Main/css")
<style>
    .table {
width: 100%;
margin-bottom: 21px;
border: 1px solid #ccc; background:#fff;
}
.table td {
text-align: center;
border: 1px solid #ccc;
}
.table tr td b {
font-size: 0.8em;
text-align: center;
}
.table tr .ng-binding {
font-size: 12px;
text-align: center;
}
.table tr td a {
font-size: 11px;
text-decoration: none;
color: inherit;
}
.table tr td a:hover {
color: #007fff;
}
.divList .btn-default {
padding: 4px 12px;
/* width: 8%; */
font-size: 12px;
background: rgba(0, 149, 255, 0.94);
margin: 0 0 10px 0;
border-radius: 19px;
color: #fff;
border: none;
}
.divList .btn-primary,.divList .btn-danger{
padding: 3px;
font-size: 12px;
}

.add_request input.btn.btn-default,.add_request input.btn.btn-danger {
padding: 2px;
font-size: 12px;
width: 10%;
}
table.table.add_request {
border: 1px solid #ccc;
}
table.table.add_request td {
border:none;    text-align: left;

}
table.table.add_request input,table.table.add_request select {
width: 100%;
}

table.table.add_request tr {
border: 1px solid #ccc;
}
table.table.add_request td b .ng-binding {
font-weight: bold;
font-size: 15px;
}
ul.table_list_userdetails {
padding: 0 0 0 17px;
margin: 0 auto;
width: 50%;
border: 1px solid #ccc;
border-radius: 5px;
background:#f5f5f5;
}
ul.table_list_userdetails li {
list-style: none;
display: inline-block;
width: 35%;
line-height: 35px;
}
ul.table_list_userdetails li span {
color: #000;
font-size: 13px;
}
.request_note ul {
padding: 0;
margin: 0;
}
.request_note ul li {
list-style: none;
line-height: 28px;
font-size: 13px;
}
.request_note h3 {
font-size: 18px;
font-weight: bold;
}
.form-control {
        -moz-box-shadow: inset 0 0 12px 17px #dadada !important;
-webkit-box-shadow: inset 0 0 1px 5px#dadada !important;
box-shadow: inset 0 0 4px 1px #dadada  !important;
height: 22px!important;
font-size: 12px!important;
}
.table select{-moz-box-shadow: inset 0 0 12px 17px #dadada !important;
-webkit-box-shadow: inset 0 0 1px 3px#dadada !important;
box-shadow: inset 0 0 2px 1px #dadada  !important;
height: 22px!important;
border-radius: 4px!important;
font-size: 12px!important;
border: 1px solid #ccc!important;
}
.btn-danger {
color: #fff;
background-color: rgba(0, 149, 255, 0.94)!important;
border-color: #d43f3a;
padding: 3px 5px!important;
font-size: 10px!important;
border:1px solid rgba(0, 149, 255, 0.94)!important;
border-radius: 4px!important;
}
.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th {
padding: 4px!important;
    
}
.divList_page_request {
height: auto!important;
overflow-y: scroll!important;
}
.table_scroll {
height: 300px;
overflow-y: scroll;
}
a.navbar-brand img {
width: 100px;
}
.navbar-inverse {
background-color:#0077bd !important;
border-color: #080808;
}
.navbar-inverse .navbar-nav>.active>a, .navbar-inverse .navbar-nav>.active>a:focus, .navbar-inverse .navbar-nav>.active>a:hover {
color: #fff;
background-color:#4e5a62 !important;
}
.navbar-nav>li>a {
padding-top: 10px!important;
padding-bottom: 9px!important;
font-size: 13px!important;
}
.navbar {
position: relative;
min-height: 40px!important;
margin-bottom: 20px;
/* border: 1px solid transparent; */
}
.navbar-brand {
float: left;
height: 39px!important;
padding: 8px 15px!important;
font-size: 18px;
line-height: 19px!important;
}
.navbar-inverse .navbar-nav>li>a {
color: #ebebeb!important;
}
.navbar-inverse .navbar-nav>.open>a, .navbar-inverse .navbar-nav>.open>a:focus, .navbar-inverse .navbar-nav>.open>a:hover {
color: #fff;
background-color: #4e5a62!important;
}
.footer {
    width: 100%;
    height: 30px;
    background: #4a87c6;
position:absolute;
bottom:0;
}
    .footer p {font-size:11px; color:#fff; line-height:30px; margin:0; padding:0; text-align:center;
    }
    .Add-right a,input.btn.btn-link.btn-sm,button.btn.btn-primary.pull-right.create_new_user{
font-size: 11px;
background: rgba(0, 149, 255, 0.94);
color: #fff;
padding: 5px 11px;
border-radius: 15px;
height: auto!important;
border:none;
line-height: initial;
}
    button.btn.btn-primary.pull-right.create_new_user { margin: 0 0 8px 0;
    }
        .Add-right a:hover { color:#000!important; background:#dadada!important;text-decoration:none!important;
        }
             
        button.btn.btn-primary.pull-right.create_new_user:hover { color:#000!important; background:#dadada!important;text-decoration:none!important;
        }
            input.btn.btn-link.btn-sm:hover {    color: #000!important; background:#dadada!important; text-decoration:none!important;
        }
    #login_page {
         background: url('../../images/loginimg.jpg') no-repeat;
    }
    .modal-body label {
    font-size: 13px;
    font-weight: normal;
}
    .modal-content .btn {
    font-size: 11px;
    padding: 5px 12px;
    border-radius: 35px;
}
    .btn.active.focus, .btn.active:focus, .btn.focus, .btn:active.focus, .btn:active:focus, .btn:focus {
        outline:none!important;
    }
</style>
<script type="text/javascript">
    //This is used to get the application's root path from javascript. It's useful if you're running application in a virtual directory under IIS.
    var abp = abp || {}; abp.appPath = '@ApplicationPath';
</script>

